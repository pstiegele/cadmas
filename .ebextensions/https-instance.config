files:  /etc/nginx/conf.d/https.conf:    mode: "000644"    owner: root    group: root    content: |      # HTTPS server      server {          listen       443;          server_name  localhost;                    ssl                  on;          ssl_certificate      /etc/pki/tls/certs/server.crt;          ssl_certificate_key  /etc/pki/tls/certs/server.key;                    ssl_session_timeout  5m;                    ssl_protocols  TLSv1 TLSv1.1 TLSv1.2;          ssl_ciphers "EECDH+AESGCM:EDH+AESGCM:AES256+EECDH:AES256+EDH";          ssl_prefer_server_ciphers   on;                    location / {              proxy_pass  https://127.0.0.1:8081;              proxy_set_header   Connection "";              proxy_http_version 1.1;              proxy_set_header        Host            $host;              proxy_set_header        X-Real-IP       $remote_addr;              proxy_set_header        X-Forwarded-For $proxy_add_x_forwarded_for;          }      }  /etc/nginx/conf.d/00_elastic_beanstalk_proxy.conf:    mode: "000644"    owner: root    group: root    content: |      # HTTP server       server {         listen       8080 default_server;         listen [::]:80 default_server;         return 301 https://$host$request_uri;       }  /etc/pki/tls/certs/server.crt:    mode: "000400"    owner: root    group: root    content: |      -----BEGIN CERTIFICATE-----      MIIDZTCCAk2gAwIBAgIJAOmRuVkc5pQYMA0GCSqGSIb3DQEBCwUAMEkxCzAJBgNV      BAYTAkRFMRMwEQYDVQQIDApTb21lLVN0YXRlMQswCQYDVQQKDAJQUzELMAkGA1UE      CwwCUFMxCzAJBgNVBAMMAlBTMB4XDTE3MTAzMTA5NDE0MVoXDTE4MTAzMTA5NDE0      MVowSTELMAkGA1UEBhMCREUxEzARBgNVBAgMClNvbWUtU3RhdGUxCzAJBgNVBAoM      AlBTMQswCQYDVQQLDAJQUzELMAkGA1UEAwwCUFMwggEiMA0GCSqGSIb3DQEBAQUA      A4IBDwAwggEKAoIBAQDdTTxtb6Cj75mGRSD6LuXCCScMd7UX+EnTonY7E1qnU1jG      p6VHnLjIFhvInmbQayP3EG+qMXrQ8LaAfUJ2G3zpW8/59Kg3MX8hvkJ2vW+bgywY      t9rf05ONG60KsCF+5ueTp2gG5LU/PXGL0EwA2/h7QxECIHqTqJuosAdV/bMMcJe+      gU9