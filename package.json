{
  "name": "cadmas",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "build": "npm run build --prefix cadmas-webclient",
    "start-local-prod": "npm install && npm install --prefix cadmas-webclient && npm run build --prefix cadmas-webclient && node ./app.js",
    "webclient": "npm start --prefix cadmas-webclient",
    "debug": "nodemon ./app.js",
    "run": "npm-run-all --parallel \"debug --inspect\" webclient  ",
    "run-no-nodemon": "npm-run-all --parallel start webclient  "
  },
  "dependencies": {
    "adm-zip": "^0.4.11",
    "aws-sdk": "^2.291.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "btoa": "^1.2.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.3",
    "npm": "^6.3.0",
    "npm-run-all": "^4.1.3",
    "request": "^2.87.0",
    "serve-favicon": "~2.4.5",
    "uuid": "^3.3.2",
    "winston": "^3.0.0",
    "ws": "^5.2.2",
    "xml2js": "^0.4.19"
  }
}
